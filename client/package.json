{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "sass:build": "sass src/assets/scss/global.scss:src/assets/scss/global.css",
    "sass:watch": "sass --watch src/assets/scss/global.scss:src/assets/scss/global.css",
    "dev": "conc 'npm run sass:watch' 'npm run vite:dev' ",
    "dev:start": "conc 'NODE_ENV=development npm run sass:watch' 'NODE_ENV=development npm run vite:dev' ",
    "dev:prod": "conc 'NODE_ENV=production npm run sass:watch' 'NODE_ENV=production vite --mode production' ",
    "build": "tsc -b && npm run sass:build && vite build",
    "test": "vitest",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router": "^6.26.2",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.10.7",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sass": "^1.79.4",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^7.1.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
