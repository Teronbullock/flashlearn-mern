{
  "name": "client",
  "private": true,
  "version": "2.0.3",
  "type": "module",
  "scripts": {
    "build:sass": "sass src/assets/scss/global.scss:src/assets/scss/global.css",
    "build:vite": "vite build",
    "build": "pnpm run build:sass && pnpm run build:vite",
    "ts-check": "tsc --noEmit",
    "watch:sass": "sass --watch src/assets/scss/global.scss:src/assets/scss/global.css",
    "test": "vitest",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:prod": "conc 'pnpm watch:sass' 'vite --mode production'",
    "dev": "conc 'pnpm run watch:sass' 'vite'"
  },
  "dependencies": {
    "@flashlearn/schema-db": "workspace:*",
    "@hookform/resolvers": "^5.2.2",
    "@tailwindcss/vite": "^4.1.13",
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "^7.71.1",
    "react-router": "^6.26.2",
    "react-router-dom": "^6.26.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "^4.3.1",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.79.4",
    "tailwindcss": "^4.1.13",
    "vite": "^7.1.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
