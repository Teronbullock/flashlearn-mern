{
  "name": "flashlearn-mern",
  "version": "1.0.1",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build:client": "pnpm -F client build",
    "build:netlify": "conc --prefix-colors 'auto' 'pnpm build:client' 'dev:netlify' ",
    "build:server": "pnpm -F server build",
    "build:common": "pnpm -F common run build",
    "build": "pnpm -F client -F server --stream build",
    "preview:client": "pnpm -F client preview",
    "preview": "pnpm --parallel -F client preview -F server --stream start",
    "db:migrate": "pnpm -F server migrate",
    "db:migrate:undo": "pnpm -F server migrate:undo",
    "dev:client": "pnpm -F client dev",
    "dev:server": "pnpm -F server dev",
    "dev:client-prod-env": "conc 'pnpm -F client dev:prod-env",
    "dev:prod-env": "pnpm --parallel -F client client-prod-env server --stream start",
    "dev:netlify": "netlify dev",
    "dev": "pnpm --parallel -F client -F server --stream dev",
    "start:server": "pnpm -F server start",
    "install:all": "pnpm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.19.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  }
}
