{
  "name": "flashlearn-mern",
  "version": "1.0.1",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "install:client": "npm --prefix ./client install",
    "install:server": "npm --prefix ./server install",
    "install:all": "concurrently \"npm run install:client\" \"npm run install:server\"",
    "db:migrate": "npm --prefix ./server run migrate",
    "db:migrate:undo": "npm --prefix ./server run migrate:undo",
    "db:migrate:undo:all": "npm --prefix ./server run migrate:undo:all",
    "build:client": "npm --prefix ./client run build",
    "build:netlify": "npm run build:client && netlify dev",
    "dev:client": "npm --prefix ./client run dev",
    "dev:client-prod-env": "npm --prefix ./client run dev:prod-env",
    "dev:server": "npm --prefix ./server run dev",
    "dev:netlify": "netlify dev",
    "dev:prod-env": "conc 'npm run dev:client-prod-env' 'npm run start:server' ",
    "dev": "conc 'npm run dev:client' 'npm run dev:server' ",
    "preview:client": "npm --prefix ./client run preview",
    "preview": "conc 'npm run preview:client' 'npm run start:server'",
    "start:server": "npm --prefix ./server run start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^9.2.1"
  }
}
