{
  "name": "server",
  "version": "1.0.1",
  "type": "module",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "migrate:users-table": "npx sequelize migration:generate --name create-users-table",
    "migrate:sets-table": "npx sequelize migration:generate --name create-sets-table",
    "migrate:cards-table": "npx sequelize migration:generate --name create-cards-table",
    "migrate:refresh-tokens-table": "npx sequelize migration:generate --name create-refresh-tokens-table",
    "migrate:all-tables": "pnpm run build:users-table && pnpm run build:sets-table && pnpm run build:cards-table && pnpm run build:refresh-tokens-table",
    "migrate:undo": "npx sequelize db:migrate:undo",
    "migrate:undo:all": "npx sequelize db:migrate:undo:all",
    "migrate": "npx sequelize db:migrate",
    "build:ts-check": "tsc --noEmit",
    "build:ts": "tsc",
    "build": "pnpm run build:ts",
    "dev": "NODE_ENV=development tsx --watch --env-file=../.env src/server.ts",
    "dev:prod-env": "NODE_ENV=production tsx --watch --env-file=../.env src/server.ts",
    "start": "pnpm run build:ts && NODE_ENV=production node dist/server.js"
  },
  "keywords": [],
  "author": "Teron Bullock",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.2",
    "common": "workspace:*",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^7.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "nanoid": "^5.1.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.3",
    "pug": "^3.0.2",
    "sequelize": "^6.3.4",
    "serverless-http": "^3.2.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/body-parser": "^1.19.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/method-override": "^3.0.0",
    "@types/node": "^22.19.3",
    "@types/pg": "^8.16.0",
    "dotenv": "^16.4.7",
    "sequelize-cli": "^6.6.2",
    "tsx": "^4.21.0"
  }
}
